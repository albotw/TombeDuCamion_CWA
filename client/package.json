{
    "name": "tombe-du-camion",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "API": "node bin/launchapi.js",
        "start": "concurrently \"npm:API\" \"npm:CLIENT\"",
        "CLIENT": "ng serve --configuration development",
        "build": "ng build",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "build:ci": "ng build --base-href /TombeDuCamion_CWA/  --source-map=false --configuration=production",
        "test:ci": "ng test --watch=false --code-coverage --source-map=false  --browsers=ChromeHeadless"
    },
