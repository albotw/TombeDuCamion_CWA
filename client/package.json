        "@angular/material": "~12.2.9",
        "@angular/platform-browser": "~12.2.0",
        "@angular/platform-browser-dynamic": "~12.2.0",
        "@angular/router": "~12.2.0",
        "@cds/core": "^5.0.0",
        "@clr/angular": "12.0.5",
        "@clr/ui": "12.0.5",
        "@types/crypto-js": "^4.1.0",
        "@types/jquery": "^3.5.8",
        "ajv": "^8.6.3",
